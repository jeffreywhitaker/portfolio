{
  "name": "vue-template",
  "version": "1.0.0",
  "description": "A template for starting Vue projects",
  "main": "index.js",
  "author": "Jeffrey Whitaker <jeffrey.david.whitaker@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "env NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "build": "env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.5",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@vue/test-utils": "^1.1.0",
    "@webpack-cli/info": "^0.2.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.5.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-vue": "^7.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.11.3",
    "prettier": "^2.1.2",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-notifier-2": "^2.1.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  }
}
